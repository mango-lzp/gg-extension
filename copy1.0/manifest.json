{
  "name": "copy-text",
  "description" : "Hello world Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "search.png"
  },
  "content_scripts": [
    {
      "js": ["content-scripts.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background":{
    "service_worker": "background.js"
  },
  "permissions":["activeTab", "storage", "scripting"]
}